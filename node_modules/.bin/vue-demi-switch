#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../pinia-plugin-persist/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
  ret=$?
else 
  node  "$basedir/../pinia-plugin-persist/node_modules/vue-demi/bin/vue-demi-switch.js" "$@"
  ret=$?
fi
exit $ret
